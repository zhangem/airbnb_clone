

<div id="edit_listing">
  <%= form_for @listing, :html => { :multipart => true } do |f| %>
    <%= f.label :name %> <br>
    <%= f.text_area :name %><br>
    <%= f.label :content %> <br>
    <%= f.text_area :content %><br>
    <%= f.hidden_field :user_id, :value => session[:user_id] %><br>
    <div class="field">
      <%= f.label :avatar %><br>
      <%= f.file_field :avatar %><br>
    </div>
    <%= f.submit(:class => 'classname') %>
  <% end %>
</div>
